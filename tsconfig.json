{
  "include": [
    "./**/*"
  ],
  "compilerOptions": {
    "strict": true,
    "types": [
      "node"
    ],
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
  }
}
